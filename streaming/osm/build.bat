python "$SUMO_HOME/tools/randomTrips.py" -n osm.net.xml --seed 42 --fringe-factor 1 -p 1.035350 -r osm.pedestrian.rou.xml -o osm.pedestrian.trips.xml -e 3600 --vehicle-class pedestrian --pedestrians --prefix ped --max-distance 2000
